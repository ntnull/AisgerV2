        } else {


            if ($('#IsSendMessage').is(':checked') == false) {
                var old_status = $('#IsSendMessage').attr('old-status');
                var c_status = $('#StatusId').val();

                if (old_status != c_status) {
                    $('#IsSendMessage').prop("checked", true);
                    $('#IsSendMessage').val(true);

